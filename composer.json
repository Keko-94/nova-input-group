{
    "name": "eom-plus/nova-input-group",
    "description": "A Laravel Nova field.",
    "keywords": [
        "laravel",
        "nova",
        "field",
        "input-group"
    ],
    "authors": [
        {
            "name": "Sebastien CRAMATTE",
            "email": "oss@operativeit.es",
            "homepage": "https://operativeit.es"
        }
    ],
    "license": "MIT",
    "require": {
        "php": "^7.3|^8.0",
        "laravel/nova": "^4.0"
    },
    "autoload": {
        "psr-4": {
            "EomPlus\\NovaInputGroup\\": "src/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "EomPlus\\NovaInputGroup\\FieldServiceProvider"
            ]
        }
    },
    "repositories": [
        {
            "type": "composer",
            "url": "https://nova.laravel.com"
        }
    ],
    "config": {
        "sort-packages": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
